{
  "name": "chesky-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "watch:css": "npx tailwindcss -i ./styles/input.css -o ./styles/output.css --watch",
    "watch:js": "next dev",
    "dev": "concurrently \"yarn watch:css\" \"yarn watch:js\"",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "tailwindcss": "^3.2.6"
  }
}
